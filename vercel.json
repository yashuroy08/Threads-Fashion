{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://ecommerce-backend-production.up.railway.app/api/:path*"
        },
        {
            "source": "/uploads/:path*",
            "destination": "https://ecommerce-backend-production.up.railway.app/uploads/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}